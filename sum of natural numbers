#include<stdio.h>
int main()
{
    int n,i,sum;
    printf("enter the number to find its sum");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
                     sum=sum+i;
                     }
                     prinf("sum of natural numbers upto %d is %d");
                     }
                     return 0;
                     }
